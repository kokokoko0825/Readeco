{
  "name": "readeco",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "EXPO_ROUTER_APP_ROOT=./app expo start --web",
    "lint": "expo lint",
    "build:web": "EXPO_ROUTER_APP_ROOT=./app pnpm exec expo export -p web && node ./scripts/copy-assets.js",
    "deploy:pages": "pnpm run build:web && wrangler pages deploy dist",
    "deploy:worker": "wrangler deploy",
    "deploy": "pnpm run deploy:pages"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@ericblade/quagga2": "^1.12.1",
    "@expo/vector-icons": "^15.0.3",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@zxing/library": "^0.21.0",
    "expo": "~54.0.27",
    "expo-auth-session": "~6.1.5",
    "expo-barcode-scanner": "^13.0.1",
    "expo-camera": "~17.0.10",
    "expo-cli": "^6.3.10",
    "expo-clipboard": "~7.0.0",
    "expo-constants": "~18.0.11",
    "expo-font": "~14.0.10",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
